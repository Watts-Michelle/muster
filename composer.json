{
	"name": "silverstripe/installer",
	"description": "API and CMS for Muster",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.5.3",
		"silverstripe/framework": "3.5.3",
		"silverstripe/reports": "3.5.3",
		"silverstripe/siteconfig": "3.5.3",
		"league/oauth2-server": "4.1.*",
		"silverstripe-australia/gridfieldextensions": "^1.4",
		"undefinedoffset/sortablegridfield": "^0.6.1",
		"unclecheese/betterbuttons": "^1.3",
		"tractorcow/silverstripe-colorpicker": "3.0.0",
		"ramsey/uuid": "^3.4",
		"unclecheese/display-logic": "1.5.0",
		"praxisnetau/silverstripe-moderno-admin": "dev-master",
		"monolog/monolog": "^1.19",
		"respect/validation": "^1.1",
		"guzzlehttp/guzzle":"5.*",
		"phpFastCache/phpFastCache": "^6.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
